<div class="nested-fields disc_field">
        <p>【<%= f.label :配送先 %>】</p>
        <div class="delivery-filed">
          <div class="col-lg-2">
            <p class="registrations-headline">郵便番号</p>
          </div>
          <div class="admin-user-delivery-filed col-lg-10">
            <%= f.text_field :delivery_postal_code, class:"form-control" %>
