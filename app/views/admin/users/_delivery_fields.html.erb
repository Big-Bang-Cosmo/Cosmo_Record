          </div>
        </div>

        <div class="delivery-filed">
          <div class="col-lg-2">
            <p class="registrations-headline">住所</p>
          </div>
          <div class="admin-user-delivery-filed  col-lg-10">
              <p><%= f.text_field :delivery_address, class:"form-control" %></p>
              <!-- <input type="text" name="delivery_address"> -->
              <!-- <input type="text" name="delivery_phone_number"> -->
          </div>
        </div>

         <br>

         <div class="remove">
            <button class="btn btn-default">
              <%= link_to_remove_association "配送先を削除", f %>
             </button>
         </div>
</div>